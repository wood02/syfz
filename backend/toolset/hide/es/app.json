{
  "identification": "syfz",
  "is_public": true,
  "name": "ES 查询",
  "version": "1.0",
  "description": "查询ES数据",
  "type": "数据库",
  "action": [
    {
      "name": "查询数据",
      "func": "scan"
    }
  ],
  "args": {
    "scan": [
      {
        "key": "host",
        "type": "text",
        "required": true
      },
      {
        "key": "port",
        "type": "number",
        "required": true,
        "default": 9200
      },
      {
        "key": "index",
        "type": "text",
        "required": true
      },
      {
        "key": "body",
        "type": "textarea",
        "required": true
      },
      {
        "key": "account",
        "type": "text",
        "required": false
      },
      {
        "key": "password",
        "type": "text",
        "required": false
      }
    ]
  }
}